{
  "name": "@weni/unnnic-system",
  "version": "3.5.2",
  "type": "commonjs",
  "files": [
    "dist",
    "src",
    "public"
  ],
  "main": "./dist/unnnic.umd.js",
  "module": "./dist/unnnic.mjs",
  "types": "./dist/index.d.ts",
