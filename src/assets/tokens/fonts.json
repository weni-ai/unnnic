{
  "font": {
    "size": {
      "value": "16px"
    },
    "family": {
      "value": "Inter"
    },
    "weight": {
      "bold": {
        "value": "600"
      },
      "semibold": {
        "value": "600"
      },
      "medium": {
        "value": "500"
      },
      "regular": {
        "value": "400"
      }
    },
    "display": {
      "1": {
        "value": "{font.weight.semibold} calc(1.5 * {font.size}) / 1.4 {font.family}"
